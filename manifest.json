{
  "manifest_version": 3,
  "name": "URL Anonymizer",
  "version": "0.1.0",
  "version_name": "0.1.0-dev.1",
  "description": "Remove tracking information from sharable links/URLs",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "omnibox": {
    "keyword": "anon"
  },
  "background": {
    "service_worker": "src/service_worker.js"
  }
}
